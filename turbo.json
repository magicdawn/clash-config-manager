{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "main#build": {
      "outputs": ["../../bundle/production/main/**"]
    },
    "ui#build": {
      "outputs": ["../../bundle/production/renderer/**"]
    }
  }
}
