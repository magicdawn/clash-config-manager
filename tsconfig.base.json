{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ESNext", "DOM"],
    "module": "preserve",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "electron", "unplugin-icons/types/react"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "useUnknownInCatchVariables": false
  }
}
